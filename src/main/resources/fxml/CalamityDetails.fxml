<?xml version="1.0" encoding="UTF-8"?>

<?import java.net.URL?>
<?import javafx.geometry.Insets?>
<?import javafx.scene.control.Button?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.control.TextArea?>
<?import javafx.scene.image.Image?>
<?import javafx.scene.image.ImageView?>
<?import javafx.scene.layout.HBox?>
<?import javafx.scene.layout.Region?>
<?import javafx.scene.layout.StackPane?>
<?import javafx.scene.layout.VBox?>
<?import javafx.scene.shape.Line?>

<StackPane prefHeight="427.0" prefWidth="762.0" xmlns="http://javafx.com/javafx/8.0.112" xmlns:fx="http://javafx.com/fxml/1">
<children>
   <VBox prefHeight="407.0" prefWidth="1000.0" spacing="10.0">
      <children>
         <HBox alignment="CENTER_RIGHT" prefHeight="27.0" prefWidth="486.0">
            <children>
                  <Button fx:id="backBtn" mnemonicParsing="false" text="&lt; Back" />
               <Region prefHeight="27.0" prefWidth="172.0" HBox.hgrow="ALWAYS" />
                  <ImageView fitHeight="56.0" fitWidth="57.0" pickOnBounds="true" preserveRatio="true">
                     <image>
                        <Image url="images/logo.png" />
                     </image>
                  </ImageView>
            </children>
         </HBox>
            <Line endX="750.0" startX="10.0" stroke="WHITE" />
            <Label fx:id="calamityTitle" styleClass="title" text="Title" textAlignment="CENTER" />
            <Label fx:id="calamityCreator" styleClass="creatorlabel" text="Creator" />
            <Label fx:id="calamityDate" styleClass="datelabel" text="Date" wrapText="true" />
            <ImageView fitHeight="284.0" fitWidth="367.0" pickOnBounds="true" preserveRatio="true" />
            <HBox minHeight="200.0" prefHeight="300.0" prefWidth="742.0">
               <children>
                  <VBox prefHeight="200.0" prefWidth="500.0">
                     <children>
                        <Label prefHeight="23.0" prefWidth="155.0" text="Description:" />
                        <Region minHeight="20.0" prefHeight="60.0" prefWidth="442.0" />
                        <TextArea fx:id="calamityDescription" editable="false" />
                     </children>
                  </VBox>
                  <Region prefHeight="200.0" prefWidth="33.0" />
                  <Line endY="200.0" stroke="WHITE" />
                  <Region prefHeight="200.0" prefWidth="24.0" HBox.hgrow="ALWAYS" />
                  <VBox prefHeight="89.0" prefWidth="243.0">
                     <children>
                        <Label text="Details:" />
                        <Region minHeight="20.0" prefHeight="29.0" prefWidth="229.0" />
                        <Label fx:id="calamityState" text="Status" />
                     </children>
                  </VBox>
               </children>
            </HBox>
      </children>
   </VBox>
</children>
<padding>
   <Insets bottom="10.0" left="10.0" right="10.0" top="10.0" />
</padding>
   <stylesheets>
      <URL value="@../css/flatterfx.css" />
      <URL value="@../css/general.css" />
   </stylesheets>
</StackPane>
