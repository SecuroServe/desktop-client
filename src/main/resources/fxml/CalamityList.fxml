<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.Insets?>
<?import javafx.scene.control.Button?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.control.TableColumn?>
<?import javafx.scene.control.TableView?>
<?import javafx.scene.layout.HBox?>
<?import javafx.scene.layout.Region?>
<?import javafx.scene.layout.StackPane?>
<?import javafx.scene.layout.VBox?>
<?import javafx.scene.text.Font?>

<StackPane xmlns="http://javafx.com/javafx/8.0.112" xmlns:fx="http://javafx.com/fxml/1" fx:controller="securoserve.ui.controller.CalamityListController">
<children>
   <VBox spacing="10.0">
      <children>
         <HBox alignment="CENTER_RIGHT" prefHeight="27.0" prefWidth="486.0">
            <children>
               <Label text="SecuroServe - Calamities" HBox.hgrow="ALWAYS">
                  <font>
                     <Font size="14.0" />
                  </font>
               </Label>
               <Region prefHeight="27.0" prefWidth="172.0" HBox.hgrow="ALWAYS" />
               <Button fx:id="closeBtn" alignment="CENTER_RIGHT" contentDisplay="CENTER" mnemonicParsing="false" style="-fx-background-color: #f44;" text="X" textAlignment="CENTER" textFill="WHITE">
                  <font>
                     <Font name="System Bold" size="10.0" />
                  </font>
                  <HBox.margin>
                     <Insets />
                  </HBox.margin>
               </Button>
            </children>
         </HBox>
            <TableView prefHeight="500.0" prefWidth="200.0">
              <columns>
                <TableColumn prefWidth="133.0" text="Calamity" />
                <TableColumn prefWidth="131.0" text="Location" />
                  <TableColumn prefWidth="145.0" text="Alerted by" />
                  <TableColumn prefWidth="76.0" text="Time" />
              </columns>
            </TableView>
            <HBox prefWidth="200.0">
               <children>
                  <Button fx:id="refreshButton" mnemonicParsing="false" text="Refresh">
                     <HBox.margin>
                        <Insets right="10.0" />
                     </HBox.margin></Button>
               </children></HBox>
      </children>
   </VBox>
</children>
<padding>
   <Insets bottom="10.0" left="10.0" right="10.0" top="10.0" />
</padding>
</StackPane>
