<?xml version="1.0" encoding="UTF-8"?>

<?import java.net.URL?>
<?import javafx.geometry.Insets?>
<?import javafx.scene.control.Button?>
<?import javafx.scene.control.TableView?>
<?import javafx.scene.layout.HBox?>
<?import javafx.scene.layout.StackPane?>
<?import javafx.scene.layout.VBox?>

<StackPane xmlns="http://javafx.com/javafx/8.0.112" xmlns:fx="http://javafx.com/fxml/1">
<children>
   <VBox spacing="10.0">
      <children>
            <Button fx:id="backBtn" mnemonicParsing="false" text="&lt; Back" />
            <TableView fx:id="calamityTable" prefHeight="500.0" prefWidth="800.0" styleClass="calamity-list" />
            <HBox prefWidth="200.0">
               <children>
                  <Button fx:id="refreshButton" mnemonicParsing="false" text="Refresh">
                     <HBox.margin>
                        <Insets right="10.0" />
                     </HBox.margin></Button>
               </children></HBox>
      </children>
   </VBox>
</children>
<padding>
   <Insets bottom="10.0" left="10.0" right="10.0" top="10.0" />
</padding>
   <stylesheets>
      <URL value="@../css/flatterfx.css" />
      <URL value="@../css/general.css" />
   </stylesheets>
</StackPane>
